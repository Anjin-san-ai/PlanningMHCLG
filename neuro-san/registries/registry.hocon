# MHCLG Agent Registry
# Lists all available agent networks for the MHCLG Planning Hub

registry {
  name = "mhclg_planning_registry"
  version = "1.0.0"
  description = "Agent networks for MHCLG Planning Hub"
  
  agent_networks = [
    {
      path = "mhclg_planning_agent.hocon"
      enabled = true
      tags = ["planning", "applications", "regulations", "uk"]
    },
    {
      path = "mhclg_summarise_agent.hocon"
      enabled = true
      tags = ["summarisation", "documents", "analysis"]
    }
  ]
  
  defaults {
    model {
      provider = "openai"
      name = "gpt-4o"
    }
    
    rate_limits {
      requests_per_minute = 60
      tokens_per_minute = 150000
    }
  }
}

